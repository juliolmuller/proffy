{
  "name": "@proffy/mobile",
  "version": "1.0.4",
  "private": true,
  "license": "MIT",
  "authors": [
    "RocketSeat",
    "Julio L. Muller"
  ],
  "main": "src/main.js",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "commit": "cd .. && npm run commit",
    "dev": "expo start",
    "development": "npm run dev",
    "build": "npm run build:android && npm run build:ios",
    "build:android": "cross-env NODE_ENV=production expo build:android --type apk",
    "build:ios": "cross-env NODE_ENV=production expo build:ios",
    "eject": "expo eject",
    "lint": "eslint src/ --fix"
  },
  "dependencies": {
    "@expo-google-fonts/archivo": "~0.3.0",
    "@expo-google-fonts/poppins": "~0.3.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.12",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.3.1",
    "axios": "^1.9.0",
    "expo": "^53.0.9",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~19.0.14",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.27.0",
    "eslint-config-any": "^1.1.0",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "^5.8.3"
  }
}
