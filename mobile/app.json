{
  "expo": {
    "name": "proffy-mobile",
    "slug": "proffy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/images/favicon.png",
    "splash": {
      "image": "./src/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9871F5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/images/favicon.png"
    }
  }
}
