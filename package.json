{
  "name": "@proffy/monorepo",
  "private": true,
  "homepage": "https://jlm-proffy.vercel.app/",
  "packageManager": "pnpm@10.6.0",
  "scripts": {
    "prepare": "husky || true",
    "commit": "commit",
    "config": "node config.js",
    "build": "npm run build --prefix server/ && npm run build --prefix web/",
    "mobile": "npm run dev --prefix mobile/",
    "server": "npm run dev --prefix server/",
    "web": "npm run dev --prefix web/",
    "lint:server": "pnpm --filter ./server/ run lint"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/config-workspace-scopes": "^19.8.0",
    "@commitlint/prompt-cli": "^19.8.0",
    "colors": "^1.4.0",
    "envfile": "^7.1.0",
    "eslint": "^9.25.1",
    "eslint-config-any": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "typescript": "^5.8.3"
  }
}
