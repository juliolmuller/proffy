{
  "name": "@proffy/server",
  "version": "auto",
  "private": true,
  "license": "MIT",
  "authors": [
    "RocketSeat",
    "Julio L. Muller"
  ],
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "commit": "cd .. && pnpm run commit",
    "prepare": "prisma generate",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "development": "tsx watch src/bootstrap.ts",
    "dev": "pnpm run development",
    "build": "rm -rf dist/ && tsc -p .",
    "production": "node dist/bootstrap.js",
    "prod": "pnpm run production",
    "start": "pnpm run production",
    "lint": "eslint --fix src/ eslint.config.mjs"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "figlet": "^1.8.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv-flow": "^3.3.3",
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.7.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.15.33",
    "eslint": "^9.29.0",
    "eslint-config-any": "^2.1.0",
    "prettier": "^3.6.1",
    "prisma": "^6.10.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
