{
  "name": "proffy-mobile",
  "version": "1.0.4",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "dev": "expo start",
    "development": "npm run dev",
    "build": "npm run build:android && npm run build:ios",
    "build:android": "cross-env NODE_ENV=production expo build:android --type apk",
    "build:ios": "cross-env NODE_ENV=production expo build:ios",
    "eject": "expo eject",
    "lint": "eslint src/ --fix"
  },
  "dependencies": {
    "@expo-google-fonts/archivo": "^0.1.0",
    "@expo-google-fonts/poppins": "^0.1.0",
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-native-picker/picker": "1.9.11",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "axios": "~0.21.1",
    "expo": "^41.0.1",
    "expo-app-loading": "^1.0.3",
    "expo-constants": "~10.1.3",
    "expo-font": "~9.1.0",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-screens": "~3.0.0",
    "react-native-web": "~0.13.18"
  },
  "devDependencies": {
    "@babel/core": "~7.9.6",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.26.0",
    "eslint-config-lacussoft": "^2.0.1",
    "expo-cli": "^4.4.7",
    "typescript": "~4.0.0"
  }
}
