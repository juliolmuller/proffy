{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "extends": "preview",
      "developmentClient": true
    },
    "preview": {
      "extends": "production",
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_PNPM": "1"
      }
    }
  }
}
