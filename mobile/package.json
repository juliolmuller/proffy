{
  "name": "@proffy/mobile",
  "version": "1.0.4",
  "private": true,
  "license": "MIT",
  "authors": [
    "RocketSeat",
    "Julio L. Muller"
  ],
  "main": "src/main.js",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "commit": "cd .. && npm run commit",
    "dev": "expo start",
    "development": "npm run dev",
    "build": "npm run build:android && npm run build:ios",
    "build:android": "cross-env NODE_ENV=production expo build:android --type apk",
    "build:ios": "cross-env NODE_ENV=production expo build:ios",
    "eject": "expo eject",
    "lint": "eslint src/ --fix"
  },
  "dependencies": {
    "@expo-google-fonts/archivo": "~0.2.0",
    "@expo-google-fonts/poppins": "~0.2.0",
    "@react-native-async-storage/async-storage": "~1.15.7",
    "@react-native-picker/picker": "1.16.1",
    "@react-navigation/bottom-tabs": "^5.11.15",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "axios": "~0.21.4",
    "expo": "^42.0.3",
    "expo-app-loading": "^1.1.2",
    "expo-constants": "~11.0.2",
    "expo-font": "~9.2.1",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-screens": "~3.4.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-lacussoft": "^2.0.3",
    "expo-cli": "^4.11.0",
    "typescript": "~4.0.0"
  }
}
