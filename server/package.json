{
  "name": "proffy-server",
  "version": "1.0.2",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "scripts": {
    "migration": "knex --knexfile knexfile.ts migrate:latest",
    "migration:up": "knex --knexfile knexfile.ts migrate:up",
    "migration:down": "knex --knexfile knexfile.ts migrate:down",
    "migration:rollback": "knex --knexfile knexfile.ts migrate:latest",
    "start": "tsnd src/index.ts --clear --respawn --transpile-only --ignore node_modules",
    "build": "rm -rf build/ && tsc -p ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "knex": "~0.21.14",
    "morgan": "^1.10.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/dotenv-flow": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
